{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "jsxImportSource": "nativewind"
  },
  "include": [
    "next-env.d.ts",
    "app-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "../shared/rnw-overrides.d.ts"
  ],
  "exclude": ["node_modules"],
  "extends": "../../tsconfig"
}
